{
  "name": "browser-workshopper",
  "version": "0.0.0",
  "description": "A browser-based node workshopper skeleton.",
  "main": "index.js",
  "bin": {
    "browser-workshopper": "index.js"
  },
  "scripts": {
    "pack": "rm -rf example/answers; rm -rf node_modules && npm install && npm dedupe && node prepack && find . -type file | grep -v workshop.tar.gz | grep -v .git | tar -cvzf ./workshop.tar.gz -T - && node postpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development node example"
  },
  "author": "Cameron Lakenen <cameron@lakenen.com> (http://lakenen.com)",
  "license": "MIT",
  "dependencies": {
    "apprise": "^1.0.0",
    "autoprefixer": "^2.1.1",
    "beefy": "^2.0.3",
    "brfs": "^1.2.0",
    "browser-workshopper-exercise": "https://github.com/lakenen/browser-workshopper-exercise/tarball/wip",
    "browser-menu": "https://github.com/lakenen/browser-menu/tarball/highlight-class",
    "browserify": "^4.2.1",
    "center-text": "^0.1.0",
    "chalk": "^0.5.1",
    "duplexer": "^0.1.1",
    "envify": "^2.0.0",
    "extend": "^1.3.0",
    "inquirer": "^0.5.1",
    "memoize-sync": "0.0.2",
    "mkdirp": "^0.5.0",
    "opener": "^1.3.0",
    "rework": "^0.20.3",
    "rework-inline": "^0.2.0",
    "rework-npm": "^0.6.1",
    "rework-plugin-ease": "^1.0.1",
    "script-load": "^0.1.0",
    "send": "^0.6.0",
    "sidenote": "^1.0.0",
    "sse-stream": "0.0.4",
    "through": "^2.3.4",
    "trumpet": "^1.6.4",
    "watchify": "^0.10.2",
    "wordwrap": "0.0.2"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/lakenen/browser-workshopper.git"
  },
