{
  "name": "browser-workshopper",
  "version": "0.0.0",
  "description": "A browser-based node workshopper skeleton.",
  "main": "index.js",
  "bin": {
    "browser-workshopper": "index.js"
  },
  "scripts": {
    "pack": "rm -rf answers; rm -rf node_modules && npm install && npm dedupe && node prepack && find . -type file | grep -v workshop.tar.gz | grep -v .git | tar -cvzf ./workshop.tar.gz -T - && node postpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "mkdir answers; cd answers && NODE_ENV=development node .."
  },
  "author": "Cameron Lakenen <cameron@lakenen.com> (http://lakenen.com)",
  "license": "MIT",
  "dependencies": {
    "apprise": "^1.0.0",
    "autoprefixer": "^1.2.0",
    "beefy": "^2.0.2",
    "brfs": "^1.1.1",
    "browser-menu": "0.0.2",
    "browserify": "^4.1.10",
    "chalk": "^0.4.0",
    "domify": "^1.2.2",
    "envify": "^1.2.1",
    "escape-html": "^1.0.1",
    "google-fonts": "0.0.0",
    "highlight.js": "^8.0.0",
    "inherits": "^2.0.1",
    "inquirer": "^0.5.1",
    "insert-css": "^0.1.1",
    "marked": "^0.3.2",
    "memoize-sync": "0.0.2",
    "mkdirp": "^0.5.0",
    "opener": "^1.3.0",
    "remove-element": "0.0.0",
    "rework": "^0.20.3",
    "rework-inline": "^0.2.0",
    "rework-npm": "^0.6.1",
    "right-now": "^1.0.0",
    "sidenote": "^1.0.0",
    "sliced": "0.0.5",
    "wordwrap": "0.0.2",
    "xhr": "^1.9.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/lakenen/browser-workshopper.git"
  },
  "bugs": {
    "url": "https://github.com/lakenen/browser-workshopper/issues"
  },
  "homepage": "https://github.com/lakenen/browser-workshopper"
}
